#here there are some rules like if mul of 3 Fizz mul of 5 Buzz if the both #FizzBuzz
def multiplire_problem(number  : int ) -> str :
    if number % 3 == 0 and number % 5 == 0 :
        return ("FizzBuzz")
    elif number % 3 == 0 :
        return ("Fizz")
    elif number % 5 == 0:
        return ("Buzz")
 

number = 15
i = 1
while i <= number :
    if i % 3 == 0 or i % 5 == 0 :
        result = multiplire_problem(i)
        print(result)
        i +=1 
    else:
        print(i)
        i +=1
